{
  "name": "shaddy-web",
  "version": "3.0.8",
  "private": true,
  "scripts": {
    "dev": "run-s dev:parallel",
    "dev:parallel": "concurrently \"pnpm build:registry:watch\" \"next dev\"",
    "build": "run-s contentlayer:build registry:build next:build",
    "start": "next start",
    "format": "prettier --config .prettierrc --log-level silent --write \"./src/**/*.{ts,tsx,mdx,json,css,scss}\"",
    "lint": "eslint",
    "lint:fix": "eslint --fix .",
    "next:build": "next build",
    "contentlayer:build": "contentlayer2 build",
    "registry:build": "tsx ./src/scripts/build-registry.ts && prettier --config .prettierrc --log-level silent --write \"./src/registry/**/*.{ts,tsx,mdx}\" --cache",
    "build:registry:watch": "tsx --watch ./src/scripts/build-registry.ts && prettier --config .prettierrc --log-level silent --write \"./src/registry/**/*.{ts,tsx,mdx}\" --cache",
    "clean": "rimraf .next .contentlayer .turbo node_modules",
    "clean:cache": "rimraf .next .turbo"
  },
  "engines": {
    "node": ">=20"
  },
