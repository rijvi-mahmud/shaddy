{
  "colors": {
    "editor.background": "#18181b",
    "editor.foreground": "#e4e4e7",
    "editor.lineHighlightBackground": "#1f1f23",
    "editor.selectionBackground": "#27272a",
    "editorCursor.foreground": "#a1a1aa",
    "editorLineNumber.foreground": "#3f3f46",
    "editorLineNumber.activeForeground": "#71717a",
    "editorIndentGuide.background": "#27272a",
    "editorIndentGuide.activeBackground": "#3f3f46",
    "editorWhitespace.foreground": "#27272a"
  },
  "displayName": "GitHub Dark",
  "name": "github-dark",
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#71717a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression"
      ],
      "settings": {
        "foreground": "#c084fc"
      }
    },
    {
      "scope": ["variable", "variable.other", "variable.parameter"],
      "settings": {
        "foreground": "#e4e4e7"
      }
    },
    {
      "scope": [
        "variable.language",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#f472b6"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#60a5fa"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#fbbf24"
      }
    },
    {
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#4ade80"
      }
    },
    {
      "scope": ["constant.numeric", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#fb923c"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#60a5fa"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#fbbf24"
      }
    },
    {
      "scope": [
        "punctuation.definition.tag",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#a1a1aa"
      }
    },
    {
      "scope": ["meta.brace", "punctuation.section"],
      "settings": {
        "foreground": "#e4e4e7"
      }
    },
    {
      "scope": "support.type.property-name",
      "settings": {
        "foreground": "#e4e4e7"
      }
    },
    {
      "scope": ["variable.other.object", "variable.other.property"],
      "settings": {
        "foreground": "#e4e4e7"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#a1a1aa"
      }
    },
    {
      "scope": [
        "storage.modifier.package",
        "storage.modifier.import",
        "meta.import"
      ],
      "settings": {
        "foreground": "#c084fc"
      }
    },
    {
      "scope": "entity.name.type.module",
      "settings": {
        "foreground": "#4ade80"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#ef4444",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#fbbf24",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#c084fc",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#60a5fa",
        "fontStyle": "bold"
      }
    }
  ],
  "type": "dark"
}
